<section class="wrapper style1 align-center">
  <div class="inner medium">
    <h2>Get in touch</h2>
    <form method="post" name="contact" netlify netlify-honeypot="bot-field">
      <div class="field half first hidden">
        <label for="name">Bot</label>
        <input type="text" name="bot-field" id="bot-field" value="" />
      </div>
      <div class="field half first">
        <label for="name">Name</label>
        <input type="text" name="name" id="name" value="" placeholder="My name is.." required />
      </div>
      <div class="field half">
        <label for="email">Email</label>
        <input type="email" name="email" id="email" value="" placeholder="My email is.." required />
      </div>
      <div class="field">
        <label for="message">Message</label>
        <textarea name="message" id="message" rows="6" placeholder="Hi. I wanted to send a message about.."></textarea>
      </div>
      <ul class="actions">
        <li>
          <input type="submit" name="submit" id="submit" value="Send Message" />
        </li>
      </ul>
    </form>

  </div>
</section>